# Lazygarde

t = int(input())
for i in range(t) :
    ok = 1
    n = int(input())
    a = sorted([int(x) for x in input().split()])
    b = sorted([int(x) for x in input().split()])
    for i in range(n) :
        if a[i] > b[i] :
            ok = 0
            break
    if ok == 1 : print("YES")
    else : print("NO")
